{
  "metadata": [
    {
      "src": [
        {
          "files": [
			"src/Wolfgang.DbContextBuilder-Core/Wolfgang.DbContextBuilder-Core.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
		  "docs/index.md",
          "README.md",
          "docs/toc.yml",
		  "docs/**/*.md",
          "api/**.yml",
          "api/toc.yml"
        ]
	  }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "src": "../"
      }
    ],
    "overwrite": [
      "docs/overrides/*.md"
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "DbContextBuilder",
      "_appFavicon": "images/favicon.ico"
    },	  
    "template": [
	  "modern",
      "default"
	]
  }
}
