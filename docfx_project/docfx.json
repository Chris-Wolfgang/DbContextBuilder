{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "DbContextBuilder/DbContextBuilder.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/toc.yml",
          "docs/*.md",
          "docs/articles/**.md",
          "api/**.yml",
          "api/toc.yaml",
          "api/index.md",
          "README.md"
        ],
        "src": "../"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "src": "../"
      }
    ],
    "overwrite": [
      "docs/overrides/*.md"
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "DbContextBuilder",
      "_appFavicon": "images/favicon.ico"
    },
    "outputFileMap": {
      "README.md": "index.html"
    },	  
    "template": [
      "default",
      "statictoc"
    ]
  }
}
